#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/rollup@4.27.4/node_modules/rollup/dist/bin/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/rollup@4.27.4/node_modules/rollup/dist/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/rollup@4.27.4/node_modules/rollup/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/rollup@4.27.4/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/rollup@4.27.4/node_modules/rollup/dist/bin/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/rollup@4.27.4/node_modules/rollup/dist/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/rollup@4.27.4/node_modules/rollup/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/rollup@4.27.4/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../dist/bin/rollup" "$@"
fi
