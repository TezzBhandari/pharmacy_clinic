#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/esbuild@0.24.0/node_modules/esbuild/bin/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/esbuild@0.24.0/node_modules/esbuild/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/esbuild@0.24.0/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/esbuild@0.24.0/node_modules/esbuild/bin/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/esbuild@0.24.0/node_modules/esbuild/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/esbuild@0.24.0/node_modules:/home/tezz/innov8ITCode/ts_projects/vanilla-js/pharmacy_template/mediaxhtml-10/download-version/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild/bin/esbuild" "$@"
fi
